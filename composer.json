{
    "name": "jfcherng/roundcube-quota-plugin",
    "description": "A plugin that shows quota information for Roundcube.",
    "type": "plugin",
    "require": {
        "php": "^5.4.0 || ^7.0.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.11.0"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix": [
            "php-cs-fixer fix --verbose"
        ]
    }
}
