{
    "name": "jfcherng/quota",
    "description": "A plugin that shows quota information for Roundcube.",
    "type": "roundcube-plugin",
    "license": "MIT",
    "homepage": "https://github.com/jfcherng/roundcube-plugin-quota",
    "version": "0.0.14",
    "authors": [
        {
            "name": "Jack Cherng",
            "email": "jfcherng@gmail.com"
        },
        {
            "name": "Victor Kirov",
            "email": "victor.kirov.eu@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://plugins.roundcube.net"
        }
    ],
    "require": {
        "php": "^5.4.0 || ^7.0.0",
        "roundcube/plugin-installer": "~0.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12.0",
        "liip/rmt": "^1.5"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix": [
            "php-cs-fixer fix --verbose"
        ]
    }
}
